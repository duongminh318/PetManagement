@model Pet

<form asp-action="Create">
    <div class="form-group">
        <label>Name</label>
        <input asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label>Age</label>
        <input asp-for="Age" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>
